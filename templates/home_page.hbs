<script>
// Function to toggle the dropdown visibility
function toggleDropdown() {
    const dropdownMenu = document.getElementById('flag-dropdown-menu');
    console.log("Toggling dropdown visibility"); // For debugging
    dropdownMenu.classList.toggle('show-dropdown');  // Toggle the visibility class
}


// Close the dropdown when clicking outside
document.addEventListener('click', function (event) {
    const flagDropdown = document.querySelector('.flag-dropdown');
    const dropdownMenu = document.getElementById('flag-dropdown-menu');
    if (!flagDropdown.contains(event.target)) {
        dropdownMenu.classList.remove('show-dropdown');
    }
});

// Set the current flag and country name based on locale
document.addEventListener('DOMContentLoaded', function () {
    const locale = 'en-gb';  // Replace with logic to fetch correct locale if needed
    const currentFlag = document.getElementById('current-flag');
    const currentCountry = document.getElementById('current-country');

    // Mapping the flags and country names based on locale
    const flagMappings = {
        'en-us': '{{asset "usflag.png"}}',
        'en-gb': '{{asset "ukflag.png"}}',
        'de': '{{asset "germanflag.png"}}',
        'sv': '{{asset "swedishflag.png"}}'
    };

    const countryNameMappings = {
        'en-us': 'English (US)',
        'en-gb': 'English (GB)',
        'de': 'Deutsch (DE)',
        'sv': 'Svenska (SE)'
    };

    // Fallback if locale is not mapped
    const flagSrc = flagMappings[locale] || '{{asset "defaultflag.png"}}';
    const countryName = countryNameMappings[locale] || 'English (Default)';

    // Inject the correct flag and country name into the DOM
    currentFlag.style.backgroundImage = `url(${flagSrc})`;  // Use backticks for template literals
    currentFlag.style.backgroundSize = 'cover';
    currentCountry.textContent = countryName;
});

// Add event listeners to each locale option to handle the URL navigation
document.querySelectorAll('.locale-option').forEach(function (localeOption) {
    localeOption.addEventListener('click', function (event) {
        event.preventDefault(); // Prevent the default link behavior
        window.location.href = localeOption.getAttribute('href'); // Navigate to the locale-specific URL
    });
});

</script>

<script>

function updateChatStatus() {
    try {
        var now = new Date(); // Get the current local time
        var ukTime = new Date(now.toLocaleString("en-GB", { timeZone: "Europe/London" })); // Convert to UK time
        var currentHour = ukTime.getHours(); // Get the current hour in UK time
        
        console.log("Current UK Time:", ukTime.toString()); // For debugging
        console.log("Current UK Hour:", currentHour); // For debugging

        var chatStatusElement = document.getElementById('chat-status');
        
        // Since HelpCenter is not defined, we'll just use 'en' as a default locale.
        var locale = 'en';  // Replace with appropriate locale logic if needed

        var statusMessages = {
            'en': { online: 'Online', offline: 'Offline' },
            'de': { online: 'Online', offline: 'Offline' },
            'sv': { online: 'Online', offline: 'Offline' }
        };

        var onlineText = statusMessages[locale].online || 'Online';
        var offlineText = statusMessages[locale].offline || 'Offline';

        // Log to see what will be set
        console.log("Online Text: " + onlineText);
        console.log("Offline Text: " + offlineText);

        if (currentHour >= 9 && currentHour < 22) {
            chatStatusElement.textContent = onlineText; // Set status to Online
            chatStatusElement.classList.add('online');
            chatStatusElement.classList.remove('offline');
            console.log("Chat status set to Online");
        } else {
            chatStatusElement.textContent = offlineText; // Set status to Offline
            chatStatusElement.classList.remove('online');
            chatStatusElement.classList.add('offline');
            console.log("Chat status set to Offline");
        }

        // Log the current status text
        console.log("Current Status Text: " + chatStatusElement.textContent);
    } catch (error) {
        console.error('Error in updateChatStatus:', error);
    }
}

// Update chat status on page load
updateChatStatus();

// Optionally, update chat status every minute
setInterval(updateChatStatus, 60000);


</script>